<mujoco model="pointfoot">
    <compiler angle="radian" meshdir="meshes/" />
    <size njmax="500" nconmax="100" />

    <default>
        <light castshadow="false" diffuse="1 1 1" />
        <camera fovy="60"/>
        <default class="visual">
            <geom contype="0" conaffinity="0" group="1" type="mesh" rgba="0.9 0.9 0.9 1.0"/>
        </default>
        <joint armature="0.01" damping="0.01"/>
        <geom contype="1" conaffinity="1" condim="3" friction="0.6 0.005 0.0001" rgba="0.5 0.6 0.7 0.0" margin="0.001" group="0" />
    </default>
    <visual>
        <rgba com="0.502 1.0 0 0.5" contactpoint="1.0 1.0 0.6 0.4" contactforce="0.2 0.98 0.2 0.8"/>
        <scale com="0.2" forcewidth="0.05" contactwidth="0.10" contactheight="0.04"/>
    </visual>

    <asset>
        <mesh name="base_link" file="base_link.STL" />
        <mesh name="abad_left_link" file="abad_left_link.STL" />
        <mesh name="hip_left_link" file="hip_left_link.STL" />
        <mesh name="knee_left_link" file="knee_left_link.STL" />
        <mesh name="foot_left_link" file="foot_left_link.STL" />
        <mesh name="abad_right_link" file="abad_right_link.STL" />
        <mesh name="hip_right_link" file="hip_right_link.STL" />
        <mesh name="knee_right_link" file="knee_right_link.STL" />
        <mesh name="foot_right_link" file="foot_right_link.STL" />
    </asset>

    <asset>
        <texture type='skybox' builtin='gradient' width='512' height='512' rgb1="1.0 1.0 1.0" rgb2="1.0 1.0 1.0"/>
        <texture name="texplane" type="2d" builtin="checker" rgb1="0.35 0.35 0.35" rgb2="0.55 0.55 0.55" width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>
        <material name="matplane" reflectance="0.0" texture="texplane" texrepeat="1 1" texuniform="true"/>
    </asset>
        <option timestep="0.001"/>

    <worldbody>
        <geom name="floor" rgba="0.5 0.6 0.7 1" pos="0 0 0" size="0 0 0.25" type="plane" material="matplane" condim="3"/>
        <camera name="track" mode="trackcom" pos="0 -2.3 1.6" xyaxes="1 0 0 0 0.707 0.707"/>
        <light directional="true" diffuse=".8 .8 .8" pos="0 0 10" dir="0 0 -10"/>

        <body name="base_Link" pos="0 0 0.63022">
            <inertial pos="-0.014551 -0.000204 -0.005893" quat="0.946589 0.00173415 -0.322428 0.00251362" mass="6.637" diaginertia="0.106053 0.0678407 0.0600956" />
            <!-- <joint name="world_Joint" pos="0 0 0" axis="1 0 0" />
            <geom type="mesh" rgba="0.79216 0.81961 0.93333 1" mesh="base_link" /> -->
            <joint type="free" limited="false" frictionloss="disable" damping="0.0000" stiffness="0.0000" armature="0"/>
            <site name="imu" pos="0 0 0"/>
            <geom class="visual" rgba="0.79216 0.81961 0.93333 1" mesh="base_link" />
            <geom name="base_collision" type="box" pos="0.017 0 -0.06" size="0.02 0.155 0.08" rgba="0 0 1 0"/>

            <body name="abad_L_Link" pos="0.0014 0.095 -0.0778">
                <inertial pos="-0.066401 0.048463 -4.2e-05" quat="0.591876 0.591731 0.384294 0.389687" mass="1.443" diaginertia="0.00187524 0.001647 0.00125177" />
                <joint name="abad_L_Joint" pos="0 0 0" axis="1 0 0" />
                <!-- <geom type="mesh" rgba="0.79216 0.81961 0.93333 1" mesh="abad_left_link" /> -->
                <geom class="visual" rgba="0.79216 0.81961 0.93333 1" mesh="abad_left_link" />

                <body name="hip_L_Link" pos="-0.071 0.019 0">
                    <inertial pos="-0.0253 -0.0506 -0.03574" quat="0.890251 0.18958 0.234293 0.341496" mass="2.099" diaginertia="0.0191449 0.0189479 0.00249317" />
                    <joint name="hip_L_Joint" pos="0 0 0" axis="0 1 0" />
                    <!-- <geom type="mesh" rgba="1 1 1 1" mesh="hip_left_link" /> -->
                    <geom class="visual" rgba="1 1 1 1" mesh="hip_left_link" />
                    <geom name="hip_L_collision" type="capsule" pos="-0.08 -0.02 -0.12" euler="0 0.55 0" size="0.025 0.12" rgba="0 0 1 0" />

                    <body name="knee_L_Link" pos="-0.15 -0.019 -0.25981">
                        <inertial pos="0.0854039 -0.00103766 -0.0433641" quat="0.714638 -0.0373799 -0.044532 0.697074" mass="0.616201" diaginertia="0.0243622 0.0220265 0.00243782" />
                        <joint name="knee_L_Joint" pos="0 0 0" axis="0 -1 0" />
                        <!-- <geom type="mesh" rgba="0.79216 0.81961 0.93333 1" mesh="knee_left_link" /> -->
                        <geom class="visual" rgba="0.79216 0.81961 0.93333 1" mesh="knee_left_link" />
                        <geom name="knee_L_collision" type="capsule" pos="0.075 0 -0.12" euler="0 -0.57 0" size="0.015 0.1" rgba ="0 0 1 0"/>

                        <!-- <geom pos="0.15 0 -0.29066" type="mesh" rgba="0.26667 0.26667 0.26667 1" mesh="foot_left_link" /> -->
                        <geom class="visual" pos="0.15 0 -0.29066" rgba="0.26667 0.26667 0.26667 1" mesh="foot_left_link" />
                        <geom name="foot_L_collision" type="sphere" pos="0.15 0 -0.26" size="0.03 0.03 0.03" rgba="0 0 1 0" />
                    </body>
                </body>
            </body>

            <body name="abad_R_Link" pos="0.0014 -0.095 -0.0778">
                <inertial pos="-0.066401 -0.048463 -4.2e-05" quat="0.389687 0.384294 0.591731 0.591876" mass="1.443" diaginertia="0.00187524 0.001647 0.00125177" />
                <joint name="abad_R_Joint" pos="0 0 0" axis="1 0 0" />
                <!-- <geom type="mesh" rgba="1 1 1 1" mesh="abad_right_link" /> -->
                <geom class="visual" rgba="0.79216 0.81961 0.93333 1" mesh="abad_right_link" />

                <body name="hip_R_Link" pos="-0.071 -0.019 0">
                    <inertial pos="-0.0253 0.0506 -0.03574" quat="0.341496 0.234293 0.18958 0.890251" mass="2.099" diaginertia="0.0191449 0.0189479 0.00249317" />
                    <joint name="hip_R_Joint" pos="0 0 0" axis="0 -1 0" />
                    <!-- <geom type="mesh" rgba="1 1 1 1" mesh="hip_right_link" /> -->
                    <geom class="visual" rgba="1 1 1 1" mesh="hip_right_link" />
                    <geom name="hip_R_collision" type="capsule" pos="-0.08 0.02 -0.12" euler="0 0.55 0" size="0.025 0.12" rgba="0 0 1 0" />

                    <body name="knee_R_Link" pos="-0.15 0.019 -0.25981">
                        <inertial pos="0.0854039 0.00103704 -0.0433641" quat="0.697077 -0.0445332 -0.0373786 0.714635" mass="0.616201" diaginertia="0.0243622 0.0220265 0.00243782" />
                        <joint name="knee_R_Joint" pos="0 0 0" axis="0 1 0" />
                        <!-- <geom type="mesh" rgba="0.79216 0.81961 0.93333 1" mesh="knee_right_link" /> -->
                        <geom class="visual" rgba="0.79216 0.81961 0.93333 1" mesh="knee_right_link" />
                        <geom name="knee_R_collision" type="capsule" pos="0.075 0 -0.12" euler="0 -0.57 0" size="0.015 0.1" rgba ="0 0 1 0" />

                        <!-- <geom pos="0.15 0 -0.29066" type="mesh" rgba="0.26667 0.26667 0.26667 1" mesh="foot_right_link"/> -->
                        <geom class="visual" pos="0.15 0 -0.29066" rgba="0.26667 0.26667 0.26667 1" mesh="foot_right_link" />
                        <geom name="foot_R_collision" type="sphere" pos="0.15 0 -0.26" size="0.03 0.03 0.03" rgba="0 0 1 0" />
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <actuator>
        <motor name="abad_L_Joint"  gear="1"  joint="abad_L_Joint" ctrllimited='true' ctrlrange='-80 80'/>
        <motor name="hip_L_Joint"   gear="1"  joint="hip_L_Joint" ctrllimited='true' ctrlrange='-80 80'/>
        <motor name="knee_L_Joint"  gear="1"  joint="knee_L_Joint" ctrllimited='true' ctrlrange='-80 80'/>
        <motor name="abad_R_Joint"  gear="1"  joint="abad_R_Joint" ctrlrange='-80 80' />
        <motor name="hip_R_Joint"   gear="1"  joint="hip_R_Joint" ctrlrange='-80 80' />
        <motor name="knee_R_Joint"  gear="1"  joint="knee_R_Joint" ctrlrange='-80 80'/>
    </actuator>

    <sensor>
        <framequat objtype="site" objname="imu"/>
        <gyro name="gyro" site="imu"/>
        <accelerometer name="acc" site="imu"/>
        <jointpos name="abad_L_Joint"    joint="abad_L_Joint" />
        <jointpos name="hip_L_Joint"     joint="hip_L_Joint" />
        <jointpos name="knee_L_Joint"    joint="knee_L_Joint" />
        <jointpos name="abad_R_Joint"    joint="abad_R_Joint" />
        <jointpos name="hip_R_Joint"     joint="hip_R_Joint" />
        <jointpos name="knee_R_Joint"    joint="knee_R_Joint" />
    </sensor>

</mujoco>
